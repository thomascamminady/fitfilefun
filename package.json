{
  "name": "fitfilefun",
  "displayName": "fitfilefun",
  "description": "Open .FIT files with CRUX.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.76.0"
  },
  "publisher": "ThomasCamminady",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomascamminady/fitfilefun.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openFitFile",
        "title": "Open .fit file with crFitTool"
      }
    ],
    "languages": [
      {
        "id": "plaintext",
        "extensions": [
          ".fit"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "fitfilefun",
      "properties": {
        "fitfilefun.fitToolPath": {
          "type": "string",
          "default": "~/Dev/crux_common/tools/crFitTool/bin/crFitTool",
          "description": "Path to the crFitTool binary"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}